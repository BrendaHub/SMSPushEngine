<mission-app>
	<objects-config>
		<mission-center>
			<init-param>
				<param-name>channel-id</param-name>
				<param-value>90</param-value>
			</init-param>
			<!--是否进行故障恢复-->
			<init-param>
			<param-name>DoFailback</param-name>
			<param-value>true</param-value>
			</init-param>	
			<!--当队列中没有任务了，wait的时间，单位为毫秒-->
			<init-param>
			<param-name>waitTime</param-name>
			<param-value>10</param-value>
			</init-param>
			<!--短信榨取线程运行数-->
			<init-param>
				<param-name>ExtractorThreadsCount</param-name>
				<param-value>1</param-value>
			</init-param>
			<!--短信榨取线程睡眠时间-->
			<init-param>
				<param-name>ExtractorProcessInterval</param-name>
				<param-value>5000</param-value>
			</init-param> 
			<!--短信处理线程运行数-->
			<init-param>
				<param-name>PopperThreadsCount</param-name>
				<param-value>2</param-value>
			</init-param>
			<!--短信处理线程睡眠时间-->
			<init-param>
				<param-name>PopperProcessInterval</param-name>
				<param-value>3990</param-value>
			</init-param>
			<!--短信回调线程运行数-->
			<init-param>
				<param-name>CallbackThreadsCount</param-name>
				<param-value>1</param-value>
			</init-param>
			<!--短信回调线程睡眠时间-->
			<init-param>
				<param-name>CallbackProcessInterval</param-name>
				<param-value>3220</param-value>
			</init-param> 
		</mission-center> 
		<!--短信榨取线程定义-->
		<extractor-thread>
			<init-param>
				<param-name>ExtractorClassName</param-name>
				<param-value>com.binggou.sms.SMSExtractor</param-value>
			</init-param>
		</extractor-thread>
		<!--短信处理线程定义-->
		<popper-thread>
			<init-param>
				<param-name>PopperClassName</param-name>
				<param-value>com.binggou.sms.SMSPopper</param-value>
			</init-param>
		</popper-thread>
		<!--短信回调线程定义-->
		<callback-thread>
			<init-param>
				<param-name>CallbackClassName</param-name>
				<param-value>com.binggou.sms.SMSCallback</param-value>
			</init-param>
		</callback-thread>
		<!--短信榨取线程-->
		<smsextractor>
			<!--一次榨取线程队列容量-->
			<init-param>
				<param-name>ExtractQueueCapability</param-name>
				<param-value>400</param-value>
			</init-param>
			<!--短信榨取线程任务总容量-->
			<init-param>
				<param-name>ExtractQuantities</param-name>
				<param-value>1000</param-value>
			</init-param>			
			<!--当短信榨取线程任务总容量满了后，可以再榨取设定优先级的最大数量,默认是200-->
			<init-param>
				<param-name>MaximumExtractQuantities</param-name>
				<param-value>200</param-value>
			</init-param>
			<!--当短信榨取线程任务总容量满了后，设定的优先级的级别，默认是1-->
			<init-param>
				<param-name>ExtractQuantitiesLevel</param-name>
				<param-value>1</param-value>
			</init-param>
		</smsextractor>
		<!--短信回调线程-->
		<smscallback>			
		</smscallback>
	</objects-config>
</mission-app>